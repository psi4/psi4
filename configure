# set up cpu specific flags
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for architecture/compiler specific optimization flags" >&5
$as_echo_n "checking for architecture/compiler specific optimization flags... " >&6; }
case $target_cpu in
  rs6000)
    if test "$FC" = xlf_r ; then
      FOPT="-qarch=com -qxflag=xalias -qdpc -qfloat=fold -qfloat=maf -qmaxmem=-1"
    fi
    if test "$CC" = xlc_r -o "$CC" = cc_r ; then
      COPT="-qarch=com -qfloat=maf -qfloat=fold -qmaxmem=-1"
    fi
    if test "$CXX" = xlC_r ; then
      CXXOPT="-qarch=com -qfloat=maf -qfloat=fold -qmaxmem=-1"
    fi
    ;;
  powerpc)
    if test "$FC" = xlf_r ; then
      FOPT="-qarch=ppc -qxflag=xalias -qdpc -qfloat=fold -qmaxmem=-1 -qfloat=maf"
    fi
    if test "$CC" = xlc_r -o "$CC" = cc_r ; then
      COPT="-qarch=ppc -qfloat=maf -qfloat=fold -qmaxmem=-1"
    fi
    if test "$CXX" = xlC_r ; then
      CXXOPT="-qarch=ppc -qfloat=maf -qfloat=fold -qmaxmem=-1"
    fi
    ;;
  i686)
    if test "$FC" = g77 ; then
      FOPT=""
    fi
    if test "$CC" = gcc -o "$CC" = cc ; then
      COPT=""
    fi
    if test "$CXX" = g++ -o "$CXX" = c++ -o "$CXX" = gcc ; then
      CXXOPT=""
    fi
    ;;
  x86_64)
    if test "$CC" = gcc -o "$CC" = cc ; then
      COPT="-m64"
    fi
    if test "$CXX" = g++ -o "$CXX" = c++ -o "$CXX" = gcc ; then
      CXXOPT="-m64"
    fi
    ;;
esac
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: C optimization flags: $COPT" >&5
$as_echo "C optimization flags: $COPT" >&6; }
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: C++ optimization flags: $CXXOPT" >&5
$as_echo "C++ optimization flags: $CXXOPT" >&6; }

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for OS specific flags" >&5
$as_echo_n "checking for OS specific flags... " >&6; }
case $target_os in
  aix*)
# _ALL_SOURCE is required because some include files on AIX don't seem to work with pure POSIX
    EXTRADEFS="-DAIX -D_LARGE_FILES -D_ALL_SOURCE"
    FDEF="$FDEF $EXTRADEFS"
    CDEF="$CDEF $EXTRADEFS"
    CXXDEF="$CXXDEF $EXTRADEFS"
    ;;
  linux*)
    EXTRADEFS="-DLinux -D_FILE_OFFSET_BITS=64 -D_LARGEFILE_SOURCE"
    FDEF="$FDEF $EXTRADEFS"
    CDEF="$CDEF $EXTRADEFS"
    CXXDEF="$CXXDEF $EXTRADEFS"
    ;;
esac
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: C preprocessor directives: $CDEF" >&5
