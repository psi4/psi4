37              =  molpro-basis-cc-pvxz-jkfit           DTQ5
97              =  basis-cc-pvxz-dk                     DTQ5
101             =  basis-cc-pwcvxz-dk                    TQ5

diffuse         =  diffuse-aug-cc-pvxz                  DTQ56
