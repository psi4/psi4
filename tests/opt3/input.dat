#! SCF cc-pVDZ geometry optimzation, with Z-matrix input

# These values are from a tightly converged QChem run
nucenergy = 9.3008002554                                                                     #TEST
refenergy = -76.027053513                                                                    #TEST

molecule h2o {
     O
     H 1 1.0
     H 1 1.0 2 104.5
# QChem optimized geometry
# O           0.000000    0.000000    0.100027
# H          -0.748790    0.000000   -0.478565
# H           0.748790    0.000000   -0.478565
}

set globals {
  diis false
  basis cc-pvdz
  e_convergence 10
  d_convergence 10
}

thisenergy = optimize('scf')

compare_values(nucenergy, h2o.nuclear_repulsion_energy(), 3, "Nuclear repulsion energy")    #TEST
compare_values(refenergy, thisenergy, 3, "Reference energy")                                #TEST
