#! cGHF STO-3G energy for the O2 molecule => z coordinate <= using a core guess, C1 symmetry, and 4-index ERI

nucenergy =    28.2227845690666648       #TEST
refenergy =  -147.63345273349796         #TEST

molecule o2 {
    0 1
    O 0.0 0.0 0.0
    O 0.0 0.0 1.2
    Symmetry C1
}

# Get a reasonable guess, to save some iterations
set = {
    reference cghf
    scf_type   pk
    basis      sto-3g
    guess core
    e_convergence 6
}
thisenergy = energy('scf')

compare_values(nucenergy, o2.nuclear_repulsion_energy(), 9, "Nuclear repulsion energy") #TEST
compare_values(refenergy, thisenergy, 9, "Reference energy")                            #TEST
compare_integers(True, psi4.variable("SCF ITERATIONS") == 15, 'cGHF N iter')             #TEST
