#! cGHF STO-3G energy for the O2 molecule => x coordinate <= using a sad guess, C1 symmetry, and 4-index ERI

nucenergy =    28.2227845690666648       #TEST
refenergy =    -147.55043891007801       #TEST

molecule o2 {
0 1
    O 0.0 0.0 0.0
    O 1.2 0.0 0.0
Symmetry C1
}

set = {
    reference cghf
    scf_type   pk
    basis      sto-3g
    guess sad
    e_convergence 1e-9
}
thisenergy = energy('scf')

compare_values(nucenergy, o2.nuclear_repulsion_energy(), 9, "Nuclear repulsion energy")
compare_values(refenergy, thisenergy, 8, "Reference energy")
compare_values(True, psi4.variable("SCF ITERATIONS") <= 16, 'cGHF N iter')
