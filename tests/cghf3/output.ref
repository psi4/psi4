#! cGHF energy for stretched H2 SAD guess, C1 symmetry, and 4-index ERI

refenergy = -0.82848721523919

molecule h2 {
0 1
    H 0 0 0
    H 0 0 3
symmetry C1
}

set {
    reference cghf
    scf_type   pk
    basis def2-SVPD
    guess sad
    diis_start 1
    diis_max_vecs 8
    e_convergence 1e-9
}
thisenergy = energy('scf')

compare_values(refenergy, thisenergy, 8, "Reference energy")                            #TEST
compare_integers(True, psi4.variable("SCF ITERATIONS") <= 15, 'cGHF N iter')             #TEST
