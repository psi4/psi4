#! ADC(2)/6-31G** on H2O using builtin ADC module

molecule h2o {
    O
    H 1 0.9584
    H 1 0.9584 2 104.45
    symmetry c1
}

set {
    reference rhf
    basis 6-31G**
    guess core
    roots_per_irrep [20]
    qc_module builtin
}

ref_energy = -76.22243196371332 
adc_energy = energy('adc(2)')
compare_values(ref_energy, adc_energy, 7, "ADC(2) GS energy")                            #TEST
