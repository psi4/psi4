#! SAPT0 cc-pVDZ computation of the ethane-ethyne binding energy, using the cc-pVDZ-JKFIT RI basis for SCF
#! and cc-pVDZ-RI for SAPT.  Monomer geometries are specified using Cartesian coordinates.

Eref = [ 85.189070701792517,  -0.003599150860,  0.003629113572,  #TEST
         -0.000831371296,     -0.001505424063, -0.002306832646 ] #TEST

molecule ethene_ethyne {
     0 1
     C     0.000000    -0.667578    -2.124659
     C     0.000000     0.667578    -2.124659
     H     0.923621    -1.232253    -2.126185
     H    -0.923621    -1.232253    -2.126185
     H    -0.923621     1.232253    -2.126185
     H     0.923621     1.232253    -2.126185
     --
     0 1
     C     0.000000     0.000000     2.900503
     C     0.000000     0.000000     1.693240
     H     0.000000     0.000000     0.627352
     H     0.000000     0.000000     3.963929
     units angstrom
     no_reorient
     symmetry c1
}

set globals {
    basis         cc-pvdz
    guess         sad
    scf_type      df
    sad_print     2
    d_converge    11
    puream        true
    print         1
}

energy('sapt0')

Eelst = PsiMod.get_variable("SAPT ELST ENERGY")
Eexch = PsiMod.get_variable("SAPT EXCH ENERGY")
Eind  = PsiMod.get_variable("SAPT IND ENERGY")
Edisp = PsiMod.get_variable("SAPT DISP ENERGY")
ET    = PsiMod.get_variable("SAPT SAPT0 ENERGY")

compare_values(Eref[0], ethene_ethyne.nuclear_repulsion_energy(), 9, "Nuclear Repulsion Energy") #TEST
compare_values(Eref[1], Eelst, 6, "SAPT0 Eelst")                                                 #TEST
compare_values(Eref[2], Eexch, 6, "SAPT0 Eexch")                                                 #TEST
compare_values(Eref[3], Eind, 6, "SAPT0 Eind")                                                   #TEST
compare_values(Eref[4], Edisp, 6, "SAPT0 Edisp")                                                 #TEST
compare_values(Eref[5], ET, 6, "SAPT0 Etotal")                                                   #TEST

clean()


