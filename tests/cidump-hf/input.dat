# FCI vector for hydrogen fluoride

molecule h2o {
0 1
F
H 1 1.5
}

set basis 6-31G
set scf_type pk

# Get SCF solution
scf_e, scf_wfn = energy('scf', return_wfn=True)

# Run valence FCI by freezing 1s
set frozen_docc [1, 0, 0, 0]
# Dump CI vectors
set dump_vectors true
# Tight convergence
set r_convergence 1e-7
energy('fci', ref_wfn = scf_wfn)

ref_vec = open('ciroot-1.dat.ref','r').read()
this_vec = open('ciroot-1.dat','r').read()
compare_cidumps(ref_vec,this_vec,"Comparing CI vectors") #TEST
