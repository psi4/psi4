#! EOM-CC3/cc-pVTZ on H$@@2$O

memory 250 mb

molecule h2o {
  0 1
# H2O Re geom from Olsen et al., JCP 104, 8007 (1996)
  O 0.0  0.00000000 -0.00900000
  H 0.0 -1.51526300 -1.05889800
  H 0.0  1.51526300 -1.05889800
  units bohr
}

set {
  wfn eom_cc3
  basis "cc-pVTZ"
  states_per_irrep [0, 0, 1, 0]
}

energy('eom-cc3')
