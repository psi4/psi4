#! Optimize H2O HF/cc-pVDZ

nucenergy =   9.3007948234239  #TEST
refenergy = -76.0270535127645  #TEST

molecule h2o {
  O 
  H 1 0.96
  H 1 0.96 2 104.5
}

if psi4.core.get_option("scf", "orbital_optimizer_package") != "INTERNAL":
    psi4.set_options({"e_convergence": 9, "d_convergence": 5e-8})

set basis cc-pVDZ
optimize('scf')

compare_values(nucenergy, h2o.nuclear_repulsion_energy(), 3, "Nuclear repulsion energy")    #TEST
compare_values(refenergy, variable("CURRENT ENERGY"), 4, "Reference energy") #TEST
