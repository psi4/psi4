#! cGHF cc-pVDZ energy for the water molecule using a core guess, C1 symmetry, and 4-index ERI

nucenergy =    9.1892221210306353     #TEST
refenergy =  -75.78008042619348       #TEST

molecule h2o {
    0 1
    O          0.00000        0.00000        0.11779
    H          0.00000        0.75545       -0.47116
    H          0.00000       -0.75545       -0.47116
    Symmetry C1
}

# Get a reasonable guess, to save some iterations
set = {
    reference cghf
    scf_type   pk
    basis      cc-pVDZ
    guess core
    e_convergence 6
}
thisenergy = energy('scf')

compare_values(nucenergy, h2o.nuclear_repulsion_energy(), 9, "Nuclear repulsion energy") #TEST
compare_values(refenergy, thisenergy, 9, "Reference energy")                             #TEST
compare_integers(True, psi4.variable("SCF ITERATIONS") == 16, 'cGHF N iter')             #TEST

