#! cc-pvdz N2 (6,6) active space Test DQG

# job description:
print('        N2 / cc-pVDZ / DQG+T2(6,6), scf_type = PK, rNN = 1.1 A')

sys.path.insert(0, '../../..')
import v2rdm_casscf

molecule n2 {
0 1
n
n 1 r
}

set {
  basis cc-pvdz
  scf_type pk
  d_convergence      1e-10
  maxiter 500
  restricted_docc [ 2, 0, 0, 0, 0, 2, 0, 0 ]
  active          [ 1, 0, 1, 1, 0, 1, 1, 1 ]
}
set v2rdm_casscf {
  positivity dqgt2
  r_convergence  1e-4
  e_convergence  5e-4
  maxiter 20000
}

activate(n2)

n2.r     = 1.1
refscf   = -108.95379624015767 # TEST
refv2rdm = -109.091487394061   # TEST

energy('v2rdm-casscf')

compare_values(refscf, variable("SCF TOTAL ENERGY"), 8, "SCF total energy") # TEST
compare_values(refv2rdm, variable("CURRENT ENERGY"), 4, "v2RDM-CASSCF total energy") # TEST

