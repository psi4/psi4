compare_values(ref_scf, variable('CURRENT REFERENCE ENERGY'), 7, 'scf ref')                        #TEST
compare_values(ref_ccsd_t_corl_loose, variable('CURRENT CORRELATION ENERGY'), 7, 'ccsd(t) corl')   #TEST
compare_values(ref_ccsd_t_tot_loose, variable('CURRENT ENERGY'), 7, 'ccsd(t) tot')                 #TEST
compare_values(ref_ccsd_t_tot_loose, val, 7, 'ccsd(t) return')                                     #TEST
clean()

