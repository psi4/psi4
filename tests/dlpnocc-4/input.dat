set basis def2-SVP
set freeze_core True
set scf_type df
set mp2_type df
set cc_type df
set nat_orbs False
set pno_convergence tight
set e_convergence 1.0e-8
set r_convergence 1.0e-8

print('   Testing DLPNO-CCSD(T) loose...')
set pno_convergence loose
val = energy('dlpno-ccsd(t)')

compare_values(ref_scf, variable('SCF TOTAL ENERGY'), 7, 'scf ref')                                #TEST
compare_values(ref_ccsd_corl_loose, variable('CCSD CORRELATION ENERGY'), 7, 'ccsd corl')           #TEST
compare_values(ref_ccsd_tot_loose, variable('CCSD TOTAL ENERGY'), 7, 'ccsd tot')                   #TEST
