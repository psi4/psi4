#compare_values(refs['dft']['muncp'] + refs['d3zero']['m'], variable('B3LYP-D2 TOTAL ENERGY'), 5, 'Ethene -D3 (calling dftd3 -zero)')  #TEST

energy('b3lyp-d3bj', molecule=mAcp)
