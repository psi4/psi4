#! Test if the the guess read in the same basis converges. 

molecule neon {
    0 3
    Ne
    Ne 1 0.5
}

set reference rohf
set basis aug-pcseg-2
set guess sad
set s_tolerance 4
set d_convergence 8
energy('scf')
compare_values(-247.03173864745776, variable('SCF TOTAL ENERGY'), 6, 'ROHF SCF energy')  #TEST

set guess read
energy('scf')
compare_values(-247.03173864745776, variable('SCF TOTAL ENERGY'), 6, 'ROHF SCF energy, read-in guess')  #TEST
niter = int(variable('SCF ITERATIONS'))
compare(1, niter, f'ROHF Iterations {niter}, read-in guess')  #TEST


set reference uhf
set guess sad
energy('scf')
compare_values(-247.03206951, variable('SCF TOTAL ENERGY'), 6, 'UHF SCF energy')  #TEST

set guess read
energy('scf')
compare_values(-247.03206951, variable('SCF TOTAL ENERGY'), 6, 'UHF SCF energy, read-in guess')  #TEST
niter = int(variable('SCF ITERATIONS'))
compare(1, niter, f'UHF Iterations {niter}, read-in guess')  #TEST
