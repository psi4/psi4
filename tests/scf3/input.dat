#! UHF 6-31G** energy of the $^3B_1$ state of CH$_2$, with Z-matrix input. The occupations
#! are specified explicitly.

nucenergy =   6.648419426607498 #TEST
refenergy = -38.91591817686461  #TEST

molecule ch2 {
    0 3
    c
    h 1 b1
    h 1 b1 2 a1

    b1 = 1.0
    a1 = 125.0
}

set globals {
    reference uhf
    basis     6-31G**
    docc      [2, 0, 0, 1]
    socc      [1, 0, 1, 0]
}

# Delete file 32 for now, because UHF has a bug in reading old MOs right now
psioh = PsiMod.IOManager.shared_object()
psioh.set_specific_retention(32, False)

thisenergy = energy('scf')

compare_values(nucenergy, ch2.nuclear_repulsion_energy(), 9, "Nuclear repulsion energy") #TEST
compare_values(refenergy, thisenergy, 9, "Reference energy")                             #TEST
