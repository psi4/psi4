#! cc-pvdz H2O Test coupled-pair LCCD against DCFT CEPA0
molecule h2o {
0 1
O
H 1 1.0 
H 1 1.0 2 104.5
}

set {
  freeze_core false
  e_convergence 1e-12
  d_convergence 1e-12
  r_convergence 1e-12
  basis cc-pvdz
}

set qc_module fnocc
energy('lccd')

corr1 = variable("LCCD CORRELATION ENERGY")

clean()

set reference       uhf
set ao_basis        none
set dcft_functional cepa0
set algorithm       twostep
energy('dcft')

corr2 = variable("DCFT LAMBDA ENERGY")

compare_values(corr1,corr2, 9, "LCCD vs DCFT/CEPA(0) correlation energy")   #TEST

clean()
