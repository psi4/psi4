#! DF-OMP3 cc-pVDZ energy for the H2O+ cation

refnuc      =  9.18738642147759 #TEST
refscf      = -75.54727960302048 #TEST
refomp3     = -75.71380673462295 #TEST

memory 256 mb

molecule h2o {
1 2
o
h 1 0.958
h 1 0.958 2 104.4776 
}

set {
  basis cc-pvdz
  df_basis_scf cc-pvdz-jkfit
  df_basis_cc cc-pvdz-ri
  scf_type df
  guess sad
  freeze_core true
  reference uhf
}
energy('df-omp3')

compare_values(refnuc, get_variable("NUCLEAR REPULSION ENERGY"), 6, "Nuclear Repulsion Energy");  #TEST
compare_values(refscf, get_variable("SCF TOTAL ENERGY"), 6, "DF-HF Energy");                        #TEST
compare_values(refomp3, get_variable("DF-OMP3 TOTAL ENERGY"), 6, "DF-OMP3 Total Energy");               #TEST


