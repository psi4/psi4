#! SCF STO-3G finite-difference frequencies from energies

memory 250 mb

molecule h2o {
  symmetry c1
  O
  H 1 0.9894093
  H 1 0.9894093 2 100.02688
}

set globals {
  basis sto-3g
  d_convergence 11
}



# Frequencies by 3-pt formula in C1 point group.
set globals { points 3 }
set findif {print 3}

frequencies('scf',dertype=0)


exit()

# Frequencies by 5-pt formula in C1 point group.
molecule h2o {
  symmetry c1
  O
  H 1 0.9894093
  H 1 0.9894093 2 100.02688
}

set globals { points 5 }

frequencies('scf',dertype=1)


clean()

