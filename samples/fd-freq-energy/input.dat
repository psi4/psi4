#! SCF STO-3G finite-difference frequencies from energies

molecule h2o {
  symmetry c1
  O
  H 1 0.9894093
  H 1 0.9894093 2 100.02688
}

set {
  basis sto-3g
  d_convergence 11
  scf_type pk
}



# Frequencies by 3-pt formula in C1 point group.
set { points 3 }
set findif {print 3}

scf_e, scf_wfn = frequencies('scf', dertype=0, return_wfn=True)



