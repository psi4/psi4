#! Test individual integral objects for correctness.
print_stdout(' Case Study Test of Integrals')

molecule h2o {
  o
  h 1 1.0
  h 1 1.0 2 103.1
}



set {
  basis sto-3g
}

wfn = Wavefunction(h2o, BasisSet.build(h2o)[0])
mints = MintsHelper(wfn.basisset())

Li = mints.so_angular_momentum()





set {
  basis 6-311G**
}

wfn = Wavefunction(h2o, BasisSet.build(h2o)[0])
mints = MintsHelper(wfn.basisset())

Li = mints.so_angular_momentum()





set {
  basis cc-pVTZ
}

wfn = Wavefunction(h2o, BasisSet.build(h2o)[0])
mints = MintsHelper(wfn.basisset())

Li = mints.so_angular_momentum()






set {
  basis sto-3g
}

wfn = Wavefunction(h2o, BasisSet.build(h2o)[0])
mints = MintsHelper(wfn.basisset())

Pi = mints.so_nabla()





