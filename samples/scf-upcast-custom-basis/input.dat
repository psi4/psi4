#! test scf castup with custom basis sets

molecule {
  O 
  H 1 0.96
  H 1 0.96 2 104.5
}

basis low {
    assign sto-3g
}
basis high {
    assign cc-pvdz
}
set scf d_convergence 6


core.IO.set_default_namespace("low")
set basis low
energy('hf')



core.IO.set_default_namespace('high')
core.IO.change_file_namespace(180, 'low', 'high')
set guess read
set basis high
energy('hf')


