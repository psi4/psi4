#! CCSD(T) cc-pVDZ geometry optimization for the H2O molecule using MRCC.

memory 500 mb

molecule h2o {
    o
    h 1 1.0
    h 1 1.0 2 104.5
}

set {
    basis cc-pvdz
    freeze_core true
}

optimize('mrccsd(t)')

compare_values(9.115333820319844, get_variable("NUCLEAR REPULSION ENERGY"), 4, 'nre')  #TEST
compare_values(-76.2413050123293, get_variable('CCSD(T) TOTAL ENERGY'), 6, 'ccsd(t)')  #TEST

