#! CEPA cc-pVDZ gradient for the NO radical

ref_vals = [                                                       #TEST
             [ 0.000000000000,  0.000000000000, -0.138608787991],  #TEST
             [ 0.000000000000,  0.000000000000,  0.138608787991]   #TEST
           ]                                                       #TEST
ref = psi4.Matrix(2, 3)                                          #TEST
ref.set(ref_vals)

memory 250 mb

molecule no {
0 2
n
o 1 1.158
}

set {
  basis cc-pvdz
  reference uhf
  guess gwh
}

grad = gradient('lccd')

compare_matrices(ref, grad, 5, "Analytic gradients")  #TEST

