#! DCFT-06 calculation for the He dimer. This performs a
#! simultaneous update of the orbitals and cumulant, using DIIS extrapolation.
#! Four-virtual integrals are handled in the MO Basis.

memory 250 mb


molecule he2 {
    He
    He 1 3.2
}

set globals {
    r_convergence 11
    ao_basis    none
    algorithm   simultaneous
    basis       6-31G**
}

energy('dcft')

