#! SCF level shift on an ROHF computation

molecule {
units bohr
0 2
B
O 1 2.277
}

set basis pcseg-1
set reference rohf
set diis false
set level_shift 0.7
set level_shift_cutoff 1e-4
set guess core
energy('scf')
compare_values(-99.49094367887790, psi4.variable("SCF TOTAL ENERGY"), 6, "SCF Energy")  #TEST
