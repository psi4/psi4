#! RHF orbitals and density for water.

molecule h2o {
0 1
#O
#H 1 R
#H 1 R 2 104.5
O@15.99491461956
H@1.00782503207  1 R
H@1.00782503207  1 R 2 104.5
}

h2o.R = 0.52917721067 / 0.52917720859

set basis cc-pvqz
set cubeprop_tasks ['esp']
set e_convergence 12
set d_convergence 12

if psi4.core.get_option("scf", "orbital_optimizer_package") != "INTERNAL":
    psi4.set_options({"e_convergence": 9, "d_convergence": 5e-9})

e, wfn = energy('scf', return_wfn=True)
cubeprop(wfn)

for s in ['Dt', 'ESP']:
    ref_cube = '%s.cube.ref' % s
    this_cube = '%s.cube' % s

