#! ADC/6-31G** on H2O

molecule h2o {
    O
    H 1 0.9584
    H 1 0.9584 2 104.45
    symmetry c1
}

set {
    reference rhf
    basis 6-31G**
    guess core
    roots_per_irrep [20]
}

ref_energy = -76.22243196371332 
adc_energy = energy('adc')
compare_values(ref_energy, adc_energy, 7, "ADC GS energy")                            #TEST
