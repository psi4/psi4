compare_values(df_scscorl, variable('SCS-MP2 CORRELATION ENERGY'), 5, 'mp2 scscorl')     #TEST
compare_values(df_scstot, variable('SCS-MP2 TOTAL ENERGY'), 5, 'mp2 scstot')             #TEST

