#! DCFT calculation for the triplet O2 using ODC-06 and ODC-12 functionals. 
#! Only simultaneous algorithm is tested.


# ODC-06


molecule OO {
0 3
O
O 1 R

units bohr
R = 2.000
}

set {
    r_convergence 12
    d_convergence 12
    ao_basis    disk
    algorithm   simultaneous
    basis       cc-pcvtz
    reference   uhf
}

set dcft_functional odc-06
energy('dcft')


set dcft_functional odc-12
energy('dcft')


