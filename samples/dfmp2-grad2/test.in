             [ 0.000000000000,    0.00000000000000,    -0.196749453151],  #TEST
             [ 0.000000000000,    0.00000000000000,     0.196749453151]   #TEST
           ]                                                             #TEST
ref = psi4.Matrix(2, 3)                                                #TEST
ref.set(ref_vals)

molecule {
