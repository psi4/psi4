# psi4PluginCache.cmake
# ---------------------
#
# This module sets some likely variable values to initialize the CMake cache in your plugin.
# See ``psi4 --plugin-compile`` for use.
#

set(CMAKE_C_COMPILER          "@CMAKE_C_COMPILER@" CACHE STRING "")
set(CMAKE_C_FLAGS             "@CMAKE_C_FLAGS@" CACHE STRING "")
set(CMAKE_CXX_COMPILER        "@CMAKE_CXX_COMPILER@" CACHE STRING "")
set(CMAKE_CXX_FLAGS           "@CMAKE_CXX_FLAGS@" CACHE STRING "")
set(CMAKE_Fortran_COMPILER    "@CMAKE_Fortran_COMPILER@" CACHE STRING "")
set(CMAKE_Fortran_FLAGS       "@CMAKE_Fortran_FLAGS@" CACHE STRING "")

#set(CMAKE_INSTALL_PREFIX      "@CMAKE_INSTALL_PREFIX@" CACHE PATH "")
set(CMAKE_INSTALL_LIBDIR      "@CMAKE_INSTALL_LIBDIR@" CACHE STRING "")
set(CMAKE_INSTALL_BINDIR      "@CMAKE_INSTALL_BINDIR@" CACHE STRING "")
set(CMAKE_INSTALL_DATADIR     "@CMAKE_INSTALL_DATADIR@" CACHE STRING "")
set(CMAKE_INSTALL_INCLUDEDIR  "@CMAKE_INSTALL_INCLUDEDIR@" CACHE STRING "")
set(PYMOD_INSTALL_LIBDIR      "@PYMOD_INSTALL_LIBDIR@" CACHE STRING "")

set(CMAKE_INSTALL_MESSAGE     "@CMAKE_INSTALL_MESSAGE@" CACHE STRING "")
set(pybind11_DIR              "@pybind11_DIR@" CACHE PATH "")

set(PYTHON_VERSION_MAJORMINOR "@PYTHON_VERSION_MAJOR@.@PYTHON_VERSION_MINOR@" CACHE STRING "")
set(PYTHON_EXECUTABLE         "@PYTHON_EXECUTABLE@" CACHE STRING "")

