#! single-point CCSDT/qz2p on NH2

print('        <<< Literal ZMAT to Cfour >>>')

cfour {
UHF-CCSDT energy calculation 
N
H 1 R
H 1 R 2 A

R=1.008
A=105.0

*ACES2(CALC=CCSDT,BASIS=qz2p
MULT=2,REF=UHF
OCCUPATION=3-1-1-0/3-0-1-0
SCF_CONV=12
CC_CONV=12
MEMORY=20000000)
}

energy('cfour')

compare_values(-55.5893469688, variable('scf total energy'), 6, 'SCF')  #TEST
compare_values(-55.784877360093, variable('mp2 total energy'), 6, 'MP2')  #TEST
compare_values(-55.808195538134, variable('ccsdt total energy'), 6, 'CCSDT')  #TEST
compare_values(-55.808195538134, variable('current energy'), 6, 'Current')  #TEST
compare_values(-0.218848569347, variable('ccsdt correlation energy'), 6, 'CCSDT corl')  #TEST

clean()
clean_variables()
cfour {}

print('        <<< Translation of ZMAT to Psi4 format to Cfour >>>')

banner('UHF-CCSDT energy calculation ')

molecule {
0 2
N
H 1 R
H 1 R 2 A

R=1.008
A=105.0
}

set {
cfour_CALC_level=CCSDT
cfour_BASIS=qz2p
cfour_REFerence=UHF
cfour_occupation [[3,1,1,0],[3,0,1,0]]
cfour_SCF_CONV=12
cfour_CC_CONV=12
}

energy('cfour')

compare_values(-55.5893469688, variable('scf total energy'), 6, 'SCF')  #TEST
