  H 1 1.0
  H 1 1.0 2 104.5
  symmetry c1
  no_reorient
  no_com
}

set {
  scf_type df
  d_convergence 8
  e_convergence 10
  mbis_radial_points 99
  mbis_spherical_points 350
  max_radial_moment 4
}

set scf_properties ['MBIS_CHARGES', 'MBIS_VOLUME_RATIOS']

e, wfn = energy('b3lyp/cc-pvdz', return_wfn=True)

