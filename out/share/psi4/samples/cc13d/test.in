#! Tests analytic CC2 gradients

molecule water{
0 1
O
H 1 R
H 1 R 2 A

R = 1.0
A = 104.5
}

set {
  reference RHF
  basis cc-pVDZ
}

analytic, wfn = gradient('cc2', return_wfn=True)

set findif points 5
fin_diff, wfn = gradient('cc2', dertype=0, return_wfn=True)

compare_matrices(analytic, fin_diff, 7, "RHF-CC2  gradient test") #TEST

