ref = psi4.Matrix.from_list([                                 #TEST
        [ 0.000000000000,  0.000000000000,  0.008667249198],  #TEST
        [ 0.000000000000,  0.000269094803, -0.004333624599],  #TEST
        [ 0.000000000000, -0.000269094803, -0.004333624599]   #TEST
