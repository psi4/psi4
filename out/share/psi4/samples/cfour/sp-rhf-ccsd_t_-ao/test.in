#! single-point CCSD(T)/qz2p on water with aobasis

print('        <<< Literal ZMAT to Cfour >>>')

cfour {
CCSD(T) single-point energy calculation using AO algorithms for ABCD terms
O
H 1 R
H 1 R 2 A

R=0.958
A=104.5

*ACES2(CALC=CCSD(T),BASIS=qz2p
ABCDTYPE=AOBASIS
SCF_CONV=12
CC_CONV=12
MEMORY=20000000)
}

energy('cfour')

compare_values(-76.0627484601, variable('scf total energy'), 6, 'SCF')  #TEST
compare_values(-76.332940127333, variable('mp2 total energy'), 6, 'MP2')  #TEST
compare_values(-76.338453951890, variable('ccsd total energy'), 6, 'CCSD')  #TEST
compare_values(-76.345717549886, variable('ccsd(t) total energy'), 6, 'CCSD(T)')  #TEST

clean()
clean_variables()
cfour {}

print('        <<< Translation of ZMAT to Psi4 format to Cfour >>>')

molecule {
O
H 1 R
H 1 R 2 A

R=0.958
A=104.5
}

set {
cfour_CALC_LEVEL=CCSD(T)
cfour_BASIS=qz2p
cfour_ABCDTYPE=AOBASIS
cfour_SCF_CONV=12
cfour_CC_CONV=12
}

energy('cfour')

