#! single-point CC3/qz2p on water

print('        <<< Literal ZMAT to Cfour >>>')

cfour {
CC3 energy calculation 
O
H 1 R
H 1 R 2 A

R=0.958
A=104.5

*ACES2(CALC=CC3,BASIS=qz2p
SCF_CONV=12
CC_CONV=12
MEMORY=20000000)

}

energy('cfour')

compare_values(-76.062748460117, variable('scf total energy'), 6, 'SCF')  #TEST
compare_values(-0.270191667216, variable('mp2 correlation energy'), 6, 'MP2 corl')  #TEST
compare_values(-0.283326618243, variable('cc3 correlation energy'), 6, 'CC3 corl')  #TEST
compare_values(-76.346075078360, variable('cc3 total energy'), 6, 'CC3')  #TEST

clean()
clean_variables()
cfour {}

print('        <<< Translation of ZMAT to Psi4 format to Cfour >>>')

molecule {
O
H 1 R
H 1 R 2 A

R=0.958
A=104.5
}

set {
cfour_CALC_level=CC3
cfour_BASIS=qz2p
cfour_SCF_CONV=12
cfour_CC_CONV=12
}

energy('cfour')

compare_values(-76.062748460117, variable('scf total energy'), 6, 'SCF')  #TEST
compare_values(-0.270191667216, variable('mp2 correlation energy'), 6, 'MP2 corl')  #TEST
compare_values(-0.283326618243, variable('cc3 correlation energy'), 6, 'CC3 corl')  #TEST
compare_values(-76.346075078360, variable('cc3 total energy'), 6, 'CC3')  #TEST

