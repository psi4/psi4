
srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

PSITARGET = $(top_objdir)/lib/libPSI_diis.a

TRUECXXSRC = \
diismanager.cc \
diisentry.cc

DEPENDINCLUDE = diisentry.h diismanager.h

LIBOBJ = $(TRUECXXSRC:%.cc=%.o)

include ../MakeRules

install_inc:: $(DEPENDINCLUDE)
	$(MKDIRS) $(DESTDIR)$(incdir)/libdiis
	$(INSTALL_INCLUDE) $^ $(DESTDIR)$(incdir)/libdiis

ifneq ($(DODEPEND),no)
include $(LIBOBJ:%.o=%.d)
endif

